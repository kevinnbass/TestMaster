{
  "api": {
    "gemini_api_key": null,
    "google_api_key": null,
    "openai_api_key": null,
    "anthropic_api_key": null,
    "rate_limit_rpm": 30,
    "timeout_seconds": 60,
    "max_retries": 3,
    "retry_delay": 2.0,
    "preferred_model": "gemini-2.5-pro",
    "fallback_models": [
      "gemini-2.0-flash",
      "gemini-1.5-pro"
    ]
  },
  "generation": {
    "max_iterations": 5,
    "self_healing_enabled": true,
    "verification_passes": 3,
    "min_quality_score": 70,
    "max_quality_score": 100,
    "parallel_workers": 4,
    "batch_size": 10,
    "use_real_imports": true,
    "generate_edge_cases": true,
    "generate_error_tests": true,
    "test_framework": "pytest",
    "coverage_target": 95.0,
    "incremental_mode": true,
    "context_window_size": 8000,
    "temperature": 0.1,
    "max_output_tokens": 4000
  },
  "monitoring": {
    "enabled": true,
    "continuous_mode": false,
    "interval_minutes": 120,
    "idle_threshold_minutes": 10,
    "watch_directories": [],
    "ignore_patterns": [
      "*.pyc",
      "__pycache__",
      ".git",
      "*.log"
    ],
    "track_refactorings": true,
    "track_renames": true,
    "track_splits": true,
    "track_merges": true,
    "similarity_threshold": 0.3,
    "change_detection_method": "git",
    "notification_channels": []
  },
  "caching": {
    "enabled": true,
    "cache_directory": "cache",
    "cache_ttl_seconds": 3600,
    "max_cache_size_mb": 500,
    "cache_strategy": "lru",
    "persistent_cache": true,
    "cache_api_responses": true,
    "cache_test_results": true,
    "cache_analysis_results": true,
    "compression_enabled": true,
    "encryption_enabled": false
  },
  "execution": {
    "parallel_execution": true,
    "max_parallel_tests": 10,
    "timeout_per_test": 30,
    "fail_fast": false,
    "retry_failed_tests": true,
    "max_test_retries": 2,
    "capture_output": true,
    "verbose_output": false,
    "profile_tests": false,
    "memory_limit_mb": 1024,
    "cpu_limit_percent": 80,
    "distributed_execution": false,
    "execution_order": "priority"
  },
  "reporting": {
    "generate_html_report": true,
    "generate_json_report": true,
    "generate_junit_xml": false,
    "report_directory": "reports",
    "include_coverage_report": true,
    "include_quality_metrics": true,
    "include_performance_metrics": true,
    "include_trend_analysis": true,
    "email_reports": false,
    "email_recipients": [],
    "slack_webhook": null,
    "dashboard_enabled": true,
    "dashboard_port": 8080
  },
  "quality": {
    "min_test_coverage": 80.0,
    "min_branch_coverage": 70.0,
    "max_cyclomatic_complexity": 10,
    "max_test_duration_seconds": 10.0,
    "require_docstrings": true,
    "require_type_hints": true,
    "lint_tests": true,
    "security_scanning": false,
    "mutation_testing": false,
    "property_based_testing": false,
    "fuzz_testing": false
  },
  "optimization": {
    "enable_test_selection": true,
    "enable_test_prioritization": true,
    "enable_deduplication": true,
    "enable_caching": true,
    "enable_parallel_generation": true,
    "optimize_imports": true,
    "minimize_api_calls": true,
    "batch_api_requests": true,
    "use_incremental_updates": true,
    "profile_performance": false,
    "auto_tune_parameters": false
  }
}